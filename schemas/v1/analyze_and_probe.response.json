{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "analyze_and_probe.response",
  "type": "object",
  "required": ["version", "data", "metadata"],
  "properties": {
    "version": {"type": "string"},
    "data": {
      "type": "object",
      "properties": {
        "structure": {"type": "object"},
        "patterns": {"type": "array"},
        "assumptions": {"type": "array"},
        "weaknesses": {"type": "array"},
        "probe_plan": {"type": "array"}
      }
    },
    "metadata": {"type": "object"},
    "error": {"type": ["object", "null"]}
  }
}


